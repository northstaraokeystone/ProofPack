{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "proofpack://receipts/ingest",
  "allOf": [{"$ref": "_base.schema.json"}],
  "properties": {
    "receipt_type": {"const": "ingest"},
    "redactions": {
      "type": "array",
      "items": {"type": "string"},
      "description": "Fields redacted before hashing"
    },
    "source_type": {
      "type": "string",
      "description": "Origin of ingested data"
    }
  },
  "required": ["source_type"]
}
